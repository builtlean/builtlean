if(type=='day'){

var isit = dt.getUTCDate();
var foo = dt;
dt.setUTCDate(dt.getUTCDate() + num);
var now = dt.getUTCDate();
//alert(isit + ":" + now + ":" + foo);
}
if(type=='month'){
dt.setUTCMonth(dt.getUTCMonth() + num);
}
