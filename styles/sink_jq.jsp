                // Copy path from File Input into Text Field
                    .change(fileChange);
        } else if (!$(this).data('fileUploadConverted')) {
            $(this)
                .data('fileUploadConverted', true)
                .change(fileChange);
        }

    });

    function fileChange() {
        var filepath = $(this).val();
