                    .css('vertical-align', verticalAlign)
                    .end()
                // Add new Browse button after File Input
                    .after('<button type="button" class="btn inf-button inf-fileupload-browse" tabindex="-1">Browse</button>')
                    .next()
                    .width(ButtonW)
