    ResizeInner(width, height);
}

function resizeHeight(height) {
    currWidth = document.body.clientWidth;
    currHeight = document.body.clientHeight;
    currHeight = document.body.clientHeight;
    endHeight = height - currHeight;
    endWidth = 0;
    window.resizeBy(endWidth, endHeight);
}

